    <div id="overlay"></div>
    <iframe id="iframeDisplay"></iframe>
    <button type="button" id="prevSlide" style="display: none"></button>
    <button type="button" id="nextSlide" style="display: none"></button>
  </body>
  <script>
    document.addEventListener("DOMContentLoaded", () => {
      let currentSlideIndex = 0;
      const slides = document.querySelectorAll(".slide");
      const progressBar = document.getElementById("progressBar");
      const showSlide = (index) => {
        slides.forEach((slide, i) => {
          slide.classList.toggle("active", i === index);
        });
        updateProgressBar(index);
      };
      const updateProgressBar = (index) => {
        const progress = ((index + 1) / slides.length) * 100;
        progressBar.style.width = progress + "%";
      };
      const onClickNextSlide = () => {
        currentSlideIndex = (currentSlideIndex + 1) % slides.length;
        showSlide(currentSlideIndex);
      };
      const onClickPrevSlide = () => {
        currentSlideIndex = (currentSlideIndex - 1 + slides.length) % slides.length;
        showSlide(currentSlideIndex);
      };
      document.getElementById("prevSlide").addEventListener("click", onClickPrevSlide);
      document.getElementById("nextSlide").addEventListener("click", onClickNextSlide);
      const iframe = document.getElementById("iframeDisplay");
      const overlay = document.getElementById("overlay");
      const showIframe = (src) => {
        iframe.src = src;
        iframe.style.display = "block";
        overlay.style.display = "block";
      };
      const clickEventHandler = () => {
        let iconClassAndUrlData = [
          { className: ".icon-8-1", url: "https://kuula.co/share/hbYGC?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-8-2", url: "https://kuula.co/share/hbY0K?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-8-3", url: "https://kuula.co/share/hbYpd?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-1", url: "https://kuula.co/share/hbP6C?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-2", url: "https://kuula.co/share/hblms?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-3", url: "https://kuula.co/share/hblhm?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-4", url: "https://kuula.co/share/hbP6b?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-5", url: "https://kuula.co/share/hbl0h?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-6", url: "https://kuula.co/share/hblp8?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-1", url: "https://kuula.co/share/hbl4B?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-2", url: "https://kuula.co/share/hbl7f?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-3", url: "https://kuula.co/share/hbft0?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-4", url: "https://kuula.co/share/hbYGZ?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-5", url: "https://kuula.co/share/hbl5d?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-1", url: "https://kuula.co/share/hbYYL?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-2", url: "https://kuula.co/share/hbPz3?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-3", url: "https://kuula.co/share/hbPw2?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-4", url: "https://kuula.co/share/hbPwL?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-5", url: "https://kuula.co/share/hbPsD?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-6", url: "https://kuula.co/share/hbPV0?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-7", url: "https://kuula.co/share/hbPCf?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-1", url: "https://kuula.co/share/hbYG4?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-2", url: "https://kuula.co/share/hbYNZ?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-3", url: "https://kuula.co/share/hbY7Q?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-4", url: "https://kuula.co/share/hbY7j?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-5", url: "https://kuula.co/share/hbPBm?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-6", url: "https://kuula.co/share/hbPS2?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-7", url: "https://kuula.co/share/hbP8P?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-1", url: "https://kuula.co/share/hbYm6?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-2", url: "https://kuula.co/share/hbYpN?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-3", url: "https://kuula.co/share/hbYfc?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-4", url: "https://kuula.co/share/hbYLn?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-5", url: "https://kuula.co/share/hbYmm?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-6", url: "https://kuula.co/share/hbYhr?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-7", url: "https://kuula.co/share/hbY5Y?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
        ];
        iconClassAndUrlData.forEach((icon) => {
          document.querySelector(icon.className).addEventListener("click", () => {
            showIframe(icon.url);
          });
        });
      };
      clickEventHandler();
      const closeIframe = (e) => {
        if (!iframe.contains(e.target) &&
          !e.target.classList.contains("icon-8-1") &&
          !e.target.classList.contains("icon-8-2") &&
          !e.target.classList.contains("icon-8-3") &&
          !e.target.classList.contains("icon-9-1") &&
          !e.target.classList.contains("icon-9-2") &&
          !e.target.classList.contains("icon-9-3") &&
          !e.target.classList.contains("icon-9-4") &&
          !e.target.classList.contains("icon-9-5") &&
          !e.target.classList.contains("icon-9-6") &&
          !e.target.classList.contains("icon-10-1") &&
          !e.target.classList.contains("icon-10-2") &&
          !e.target.classList.contains("icon-10-3") &&
          !e.target.classList.contains("icon-10-4") &&
          !e.target.classList.contains("icon-10-5") &&
          !e.target.classList.contains("icon-11-1") &&
          !e.target.classList.contains("icon-11-2") &&
          !e.target.classList.contains("icon-11-3") &&
          !e.target.classList.contains("icon-11-4") &&
          !e.target.classList.contains("icon-11-5") &&
          !e.target.classList.contains("icon-11-6") &&
          !e.target.classList.contains("icon-11-7") &&
          !e.target.classList.contains("icon-12-1") &&
          !e.target.classList.contains("icon-12-2") &&
          !e.target.classList.contains("icon-12-3") &&
          !e.target.classList.contains("icon-12-4") &&
          !e.target.classList.contains("icon-12-5") &&
          !e.target.classList.contains("icon-12-6") &&
          !e.target.classList.contains("icon-12-7") &&
          !e.target.classList.contains("icon-13-1") &&
          !e.target.classList.contains("icon-13-2") &&
          !e.target.classList.contains("icon-13-3") &&
          !e.target.classList.contains("icon-13-4") &&
          !e.target.classList.contains("icon-13-5") &&
          !e.target.classList.contains("icon-13-6") &&
          !e.target.classList.contains("icon-13-7") &&
          true) {
          iframe.style.display = "none";
          overlay.style.display = "none";
        }
      };
      window.addEventListener("click", closeIframe);
      window.addEventListener("touchstart", closeIframe);
      window.addEventListener("keydown", (e) => {
        if (e.key === "ArrowLeft") { onClickPrevSlide(); }
      });
      window.addEventListener("keydown", (e) => {
        if (e.key === "ArrowRight") { onClickNextSlide(); }
      });
      let startX;
      const handleTouchStart = (e) => { startX = e.touches[0].clientX; };
      const handleTouchEnd = (e) => {
        if (!startX) return;
        let endX = e.changedTouches[0].clientX;
        let diffX = startX - endX;
        if (diffX > 50) { onClickNextSlide(); }
        else if (diffX < -50) { onClickPrevSlide(); }
        startX = null;
      };
      document.addEventListener("touchstart", handleTouchStart, false);
      document.addEventListener("touchend", handleTouchEnd, false);
      updateProgressBar(currentSlideIndex);
    });
  </script>
