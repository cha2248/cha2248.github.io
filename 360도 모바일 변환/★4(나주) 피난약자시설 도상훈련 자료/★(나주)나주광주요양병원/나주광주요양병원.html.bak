    <div id="overlay"></div>
    <iframe id="iframeDisplay"></iframe>
    <button type="button" id="prevSlide" style="display: none"></button>
    <button type="button" id="nextSlide" style="display: none"></button>
  </body>
  <script>
    document.addEventListener("DOMContentLoaded", () => {
      let currentSlideIndex = 0;
      const slides = document.querySelectorAll(".slide");
      const progressBar = document.getElementById("progressBar");
      const showSlide = (index) => {
        slides.forEach((slide, i) => {
          slide.classList.toggle("active", i === index);
        });
        updateProgressBar(index);
      };
      const updateProgressBar = (index) => {
        const progress = ((index + 1) / slides.length) * 100;
        progressBar.style.width = progress + "%";
      };
      const onClickNextSlide = () => {
        currentSlideIndex = (currentSlideIndex + 1) % slides.length;
        showSlide(currentSlideIndex);
      };
      const onClickPrevSlide = () => {
        currentSlideIndex = (currentSlideIndex - 1 + slides.length) % slides.length;
        showSlide(currentSlideIndex);
      };
      document.getElementById("prevSlide").addEventListener("click", onClickPrevSlide);
      document.getElementById("nextSlide").addEventListener("click", onClickNextSlide);
      const iframe = document.getElementById("iframeDisplay");
      const overlay = document.getElementById("overlay");
      const showIframe = (src) => {
        iframe.src = src;
        iframe.style.display = "block";
        overlay.style.display = "block";
      };
      const clickEventHandler = () => {
        let iconClassAndUrlData = [
          { className: ".icon-18-1", url: "https://kuula.co/share/hZs5V?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-2", url: "https://kuula.co/share/hZs5y?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-3", url: "https://kuula.co/share/hZs5Q?logo=0&info=1&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-4", url: "https://kuula.co/share/hZshp?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-5", url: "https://kuula.co/share/hZshC?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-6", url: "https://kuula.co/share/hZsLJ?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-7", url: "https://kuula.co/share/hZsLH?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-8", url: "https://kuula.co/share/hZsf9?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-9", url: "https://kuula.co/share/hZsfM?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-10", url: "https://kuula.co/share/hZsl4?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-19-1", url: "https://kuula.co/share/hZshD?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-19-2", url: "https://kuula.co/share/hZshc?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-19-3", url: "https://kuula.co/share/hZshK?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-19-4", url: "https://kuula.co/share/hZshJ?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-19-5", url: "https://kuula.co/share/hZshx?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-19-6", url: "https://kuula.co/share/hZsh8?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-20-1", url: "https://kuula.co/share/hZs0c?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-20-2", url: "https://kuula.co/share/hZs0t?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-20-3", url: "https://kuula.co/share/hZsGp?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-20-4", url: "https://kuula.co/share/hZsGk?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-20-5", url: "https://kuula.co/share/hZsGx?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-20-6", url: "https://kuula.co/share/hZsfp?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-21-1", url: "https://kuula.co/share/hZs0f?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-21-2", url: "https://kuula.co/share/hZspq?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-21-3", url: "https://kuula.co/share/hZsp6?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-21-4", url: "https://kuula.co/share/hZs0p?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-21-5", url: "https://kuula.co/share/hZs00?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-22-1", url: "https://kuula.co/share/hZsmR?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-22-2", url: "https://kuula.co/share/hZsmW?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-22-3", url: "https://kuula.co/share/hZspf?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-23-1", url: "https://kuula.co/share/hZsLj?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-23-2", url: "https://kuula.co/share/hZsm7?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-23-3", url: "https://kuula.co/share/hZsL2?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-23-4", url: "https://kuula.co/share/hZsLt?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-23-5", url: "https://kuula.co/share/hZsmh?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-23-6", url: "https://kuula.co/share/hZsmw?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
        ];
        iconClassAndUrlData.forEach((icon) => {
          document.querySelector(icon.className).addEventListener("click", () => {
            showIframe(icon.url);
          });
        });
      };
      clickEventHandler();
      const closeIframe = (e) => {
        if (!iframe.contains(e.target) &&
          !e.target.classList.contains("icon-18-1") &&
          !e.target.classList.contains("icon-18-2") &&
          !e.target.classList.contains("icon-18-3") &&
          !e.target.classList.contains("icon-18-4") &&
          !e.target.classList.contains("icon-18-5") &&
          !e.target.classList.contains("icon-18-6") &&
          !e.target.classList.contains("icon-18-7") &&
          !e.target.classList.contains("icon-18-8") &&
          !e.target.classList.contains("icon-18-9") &&
          !e.target.classList.contains("icon-18-10") &&
          !e.target.classList.contains("icon-19-1") &&
          !e.target.classList.contains("icon-19-2") &&
          !e.target.classList.contains("icon-19-3") &&
          !e.target.classList.contains("icon-19-4") &&
          !e.target.classList.contains("icon-19-5") &&
          !e.target.classList.contains("icon-19-6") &&
          !e.target.classList.contains("icon-20-1") &&
          !e.target.classList.contains("icon-20-2") &&
          !e.target.classList.contains("icon-20-3") &&
          !e.target.classList.contains("icon-20-4") &&
          !e.target.classList.contains("icon-20-5") &&
          !e.target.classList.contains("icon-20-6") &&
          !e.target.classList.contains("icon-21-1") &&
          !e.target.classList.contains("icon-21-2") &&
          !e.target.classList.contains("icon-21-3") &&
          !e.target.classList.contains("icon-21-4") &&
          !e.target.classList.contains("icon-21-5") &&
          !e.target.classList.contains("icon-22-1") &&
          !e.target.classList.contains("icon-22-2") &&
          !e.target.classList.contains("icon-22-3") &&
          !e.target.classList.contains("icon-23-1") &&
          !e.target.classList.contains("icon-23-2") &&
          !e.target.classList.contains("icon-23-3") &&
          !e.target.classList.contains("icon-23-4") &&
          !e.target.classList.contains("icon-23-5") &&
          !e.target.classList.contains("icon-23-6") &&
          true) {
          iframe.style.display = "none";
          overlay.style.display = "none";
        }
      };
      window.addEventListener("click", closeIframe);
      window.addEventListener("touchstart", closeIframe);
      window.addEventListener("keydown", (e) => {
        if (e.key === "ArrowLeft") { onClickPrevSlide(); }
      });
      window.addEventListener("keydown", (e) => {
        if (e.key === "ArrowRight") { onClickNextSlide(); }
      });
      let startX;
      const handleTouchStart = (e) => { startX = e.touches[0].clientX; };
      const handleTouchEnd = (e) => {
        if (!startX) return;
        let endX = e.changedTouches[0].clientX;
        let diffX = startX - endX;
        if (diffX > 50) { onClickNextSlide(); }
        else if (diffX < -50) { onClickPrevSlide(); }
        startX = null;
      };
      document.addEventListener("touchstart", handleTouchStart, false);
      document.addEventListener("touchend", handleTouchEnd, false);
      updateProgressBar(currentSlideIndex);
    });
  </script>
