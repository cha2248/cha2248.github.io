          { className: ".icon-17-3", url: "https://kuula.co/share/hbdBt?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-17-4", url: "https://kuula.co/share/hbdrJ?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-17-5", url: "https://kuula.co/share/hbdSM?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
