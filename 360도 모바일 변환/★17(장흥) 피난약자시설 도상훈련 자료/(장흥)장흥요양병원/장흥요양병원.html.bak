          { className: ".icon-12-1", url: "https://kuula.co/share/hbn7S?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-2", url: "https://kuula.co/share/hbnm0?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-1", url: "https://kuula.co/share/hbd1M?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-2", url: "https://kuula.co/share/hbdKj?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-3", url: "https://kuula.co/share/hbdYq?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-4", url: "https://kuula.co/share/hbdcL?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-5", url: "https://kuula.co/share/hbn5h?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-6", url: "https://kuula.co/share/hbdbb?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-7", url: "https://kuula.co/share/hbnNZ?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-8", url: "https://kuula.co/share/hbn0f?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-9", url: "https://kuula.co/share/hZsKL?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-10", url: "https://kuula.co/share/hbn5n?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-14-1", url: "https://kuula.co/share/hbdV2?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
