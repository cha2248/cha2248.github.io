          { className: ".icon-14-7", url: "https://kuula.co/share/hbdxF?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-15-1", url: "https://kuula.co/share/hbdsv?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-15-2", url: "https://kuula.co/share/hbdsy?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-15-3", url: "https://kuula.co/share/hbdwG?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-15-4", url: "https://kuula.co/share/hbd3M?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-15-5", url: "https://kuula.co/share/hbd20?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-16-1", url: "https://kuula.co/share/hbdWZ?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-16-2", url: "https://kuula.co/share/hbdWV?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-16-3", url: "https://kuula.co/share/hbdrJ?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
