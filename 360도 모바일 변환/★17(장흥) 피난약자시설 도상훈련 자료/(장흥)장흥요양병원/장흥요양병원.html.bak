          { className: ".icon-18-2", url: "https://kuula.co/share/hbdtp?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-18-3", url: "https://kuula.co/share/hbdtX?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-19-1", url: "https://kuula.co/share/hbn5H?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-19-2", url: "https://kuula.co/share/hbn5b?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
        ];
        iconClassAndUrlData.forEach((icon) => {
          document.querySelector(icon.className).addEventListener("click", () => {
            showIframe(icon.url);
          });
        });
      };
      clickEventHandler();
      const closeIframe = (e) => {
        if (!iframe.contains(e.target) &&
          !e.target.classList.contains("icon-5-1") &&
          !e.target.classList.contains("icon-5-2") &&
          !e.target.classList.contains("icon-5-3") &&
          !e.target.classList.contains("icon-5-4") &&
          !e.target.classList.contains("icon-6-1") &&
          !e.target.classList.contains("icon-9-1") &&
          !e.target.classList.contains("icon-12-1") &&
          !e.target.classList.contains("icon-12-2") &&
          !e.target.classList.contains("icon-13-1") &&
          !e.target.classList.contains("icon-13-2") &&
          !e.target.classList.contains("icon-13-3") &&
          !e.target.classList.contains("icon-13-4") &&
          !e.target.classList.contains("icon-13-5") &&
          !e.target.classList.contains("icon-13-6") &&
          !e.target.classList.contains("icon-13-7") &&
          !e.target.classList.contains("icon-13-8") &&
          !e.target.classList.contains("icon-13-9") &&
          !e.target.classList.contains("icon-13-10") &&
          !e.target.classList.contains("icon-14-1") &&
          !e.target.classList.contains("icon-14-2") &&
          !e.target.classList.contains("icon-14-3") &&
          !e.target.classList.contains("icon-14-4") &&
          !e.target.classList.contains("icon-14-5") &&
          !e.target.classList.contains("icon-14-6") &&
          !e.target.classList.contains("icon-14-7") &&
          !e.target.classList.contains("icon-15-1") &&
          !e.target.classList.contains("icon-15-2") &&
          !e.target.classList.contains("icon-15-3") &&
          !e.target.classList.contains("icon-15-4") &&
          !e.target.classList.contains("icon-15-5") &&
          !e.target.classList.contains("icon-16-1") &&
          !e.target.classList.contains("icon-16-2") &&
          !e.target.classList.contains("icon-16-3") &&
          !e.target.classList.contains("icon-16-4") &&
          !e.target.classList.contains("icon-16-5") &&
          !e.target.classList.contains("icon-17-1") &&
          !e.target.classList.contains("icon-17-2") &&
          !e.target.classList.contains("icon-17-3") &&
          !e.target.classList.contains("icon-17-4") &&
          !e.target.classList.contains("icon-17-5") &&
          !e.target.classList.contains("icon-18-1") &&
          !e.target.classList.contains("icon-18-2") &&
          !e.target.classList.contains("icon-18-3") &&
          !e.target.classList.contains("icon-19-1") &&
          !e.target.classList.contains("icon-19-2") &&
          true) {
          iframe.style.display = "none";
          overlay.style.display = "none";
        }
      };
      window.addEventListener("click", closeIframe);
      window.addEventListener("touchstart", closeIframe);
      window.addEventListener("keydown", (e) => {
        if (e.key === "ArrowLeft") { onClickPrevSlide(); }
      });
      window.addEventListener("keydown", (e) => {
        if (e.key === "ArrowRight") { onClickNextSlide(); }
      });
      let startX;
      const handleTouchStart = (e) => { startX = e.touches[0].clientX; };
      const handleTouchEnd = (e) => {
        if (!startX) return;
        let endX = e.changedTouches[0].clientX;
        let diffX = startX - endX;
        if (diffX > 50) { onClickNextSlide(); }
        else if (diffX < -50) { onClickPrevSlide(); }
        startX = null;
      };
      document.addEventListener("touchstart", handleTouchStart, false);
      document.addEventListener("touchend", handleTouchEnd, false);
      updateProgressBar(currentSlideIndex);
    });
  </script>
</html>
