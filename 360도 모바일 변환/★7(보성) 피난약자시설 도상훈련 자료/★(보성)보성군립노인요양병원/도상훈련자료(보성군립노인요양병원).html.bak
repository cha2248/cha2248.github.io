    <div id="overlay"></div>
    <iframe id="iframeDisplay"></iframe>
    <button type="button" id="prevSlide" style="display: none"></button>
    <button type="button" id="nextSlide" style="display: none"></button>
  </body>
  <script>
    document.addEventListener("DOMContentLoaded", () => {
      let currentSlideIndex = 0;
      const slides = document.querySelectorAll(".slide");
      const progressBar = document.getElementById("progressBar");
      const showSlide = (index) => {
        slides.forEach((slide, i) => {
          slide.classList.toggle("active", i === index);
        });
        updateProgressBar(index);
      };
      const updateProgressBar = (index) => {
        const progress = ((index + 1) / slides.length) * 100;
        progressBar.style.width = progress + "%";
      };
      const onClickNextSlide = () => {
        currentSlideIndex = (currentSlideIndex + 1) % slides.length;
        showSlide(currentSlideIndex);
      };
      const onClickPrevSlide = () => {
        currentSlideIndex = (currentSlideIndex - 1 + slides.length) % slides.length;
        showSlide(currentSlideIndex);
      };
      document.getElementById("prevSlide").addEventListener("click", onClickPrevSlide);
      document.getElementById("nextSlide").addEventListener("click", onClickNextSlide);
      const iframe = document.getElementById("iframeDisplay");
      const overlay = document.getElementById("overlay");
      const showIframe = (src) => {
        iframe.src = src;
        iframe.style.display = "block";
        overlay.style.display = "block";
      };
      const clickEventHandler = () => {
        let iconClassAndUrlData = [
          { className: ".icon-4-1", url: "https://kuula.co/share/hZZNM?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-4-2", url: "https://kuula.co/share/hZZN8?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-4-3", url: "https://kuula.co/share/hZZNy?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-4-4", url: "https://kuula.co/share/hZZm4?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-8-1", url: "https://kuula.co/share/hZZpm?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-8-2", url: "https://kuula.co/share/hZZpv?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-8-3", url: "https://kuula.co/share/hZZpl?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-1", url: "https://kuula.co/share/hZZLn?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-2", url: "https://kuula.co/share/hZZfm?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-3", url: "https://kuula.co/share/hZ9m7?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-4", url: "https://kuula.co/share/hZZk0?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-9-5", url: "https://kuula.co/share/hZZY2?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-1", url: "https://kuula.co/share/hZZky?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-2", url: "https://kuula.co/share/hZZ99?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-3", url: "https://kuula.co/share/hZZF9?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-4", url: "https://kuula.co/share/hZZJ0?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-5", url: "https://kuula.co/share/hZZX4?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-6", url: "https://kuula.co/share/hZZXK?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-10-7", url: "https://kuula.co/share/hZZZG?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-1", url: "https://kuula.co/share/hZZK9?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-2", url: "https://kuula.co/share/hZZZC?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-3", url: "https://kuula.co/share/hZZKD?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-4", url: "https://kuula.co/share/hZZb3?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-5", url: "https://kuula.co/share/hZZ1G?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-6", url: "https://kuula.co/share/hZZ1G?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-7", url: "https://kuula.co/share/hZZDr?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-11-8", url: "https://kuula.co/share/hZZHZ?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-1", url: "https://kuula.co/share/hZZMy?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-2", url: "https://kuula.co/share/hZZMy?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-3", url: "https://kuula.co/share/hZZng?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-4", url: "https://kuula.co/share/hZZMy?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-5", url: "https://kuula.co/share/hZZdb?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-6", url: "https://kuula.co/share/hZZnp?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-12-7", url: "https://kuula.co/share/hZZWw?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-1", url: "https://kuula.co/share/hZZ4z?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
          { className: ".icon-13-2", url: "https://kuula.co/share/hZZNY?logo=0&info=0&fs=1&vr=1&sd=1&thumbs=1" },
        ];
        iconClassAndUrlData.forEach((icon) => {
          document.querySelector(icon.className).addEventListener("click", () => {
            showIframe(icon.url);
          });
        });
      };
      clickEventHandler();
      const closeIframe = (e) => {
        if (!iframe.contains(e.target) &&
          !e.target.classList.contains("icon-4-1") &&
          !e.target.classList.contains("icon-4-2") &&
          !e.target.classList.contains("icon-4-3") &&
          !e.target.classList.contains("icon-4-4") &&
          !e.target.classList.contains("icon-8-1") &&
          !e.target.classList.contains("icon-8-2") &&
          !e.target.classList.contains("icon-8-3") &&
          !e.target.classList.contains("icon-9-1") &&
          !e.target.classList.contains("icon-9-2") &&
          !e.target.classList.contains("icon-9-3") &&
          !e.target.classList.contains("icon-9-4") &&
          !e.target.classList.contains("icon-9-5") &&
          !e.target.classList.contains("icon-10-1") &&
          !e.target.classList.contains("icon-10-2") &&
          !e.target.classList.contains("icon-10-3") &&
          !e.target.classList.contains("icon-10-4") &&
          !e.target.classList.contains("icon-10-5") &&
          !e.target.classList.contains("icon-10-6") &&
          !e.target.classList.contains("icon-10-7") &&
          !e.target.classList.contains("icon-11-1") &&
          !e.target.classList.contains("icon-11-2") &&
          !e.target.classList.contains("icon-11-3") &&
          !e.target.classList.contains("icon-11-4") &&
          !e.target.classList.contains("icon-11-5") &&
          !e.target.classList.contains("icon-11-6") &&
          !e.target.classList.contains("icon-11-7") &&
          !e.target.classList.contains("icon-11-8") &&
          !e.target.classList.contains("icon-12-1") &&
          !e.target.classList.contains("icon-12-2") &&
          !e.target.classList.contains("icon-12-3") &&
          !e.target.classList.contains("icon-12-4") &&
          !e.target.classList.contains("icon-12-5") &&
          !e.target.classList.contains("icon-12-6") &&
          !e.target.classList.contains("icon-12-7") &&
          !e.target.classList.contains("icon-13-1") &&
          !e.target.classList.contains("icon-13-2") &&
          true) {
          iframe.style.display = "none";
          overlay.style.display = "none";
        }
      };
      window.addEventListener("click", closeIframe);
      window.addEventListener("touchstart", closeIframe);
      window.addEventListener("keydown", (e) => {
        if (e.key === "ArrowLeft") { onClickPrevSlide(); }
      });
      window.addEventListener("keydown", (e) => {
        if (e.key === "ArrowRight") { onClickNextSlide(); }
      });
      let startX;
      const handleTouchStart = (e) => { startX = e.touches[0].clientX; };
      const handleTouchEnd = (e) => {
        if (!startX) return;
        let endX = e.changedTouches[0].clientX;
        let diffX = startX - endX;
        if (diffX > 50) { onClickNextSlide(); }
        else if (diffX < -50) { onClickPrevSlide(); }
        startX = null;
      };
      document.addEventListener("touchstart", handleTouchStart, false);
      document.addEventListener("touchend", handleTouchEnd, false);
      updateProgressBar(currentSlideIndex);
    });
  </script>
